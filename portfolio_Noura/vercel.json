{
    "builds": [
      {
        "src": "portfolio_Noura/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        }
      }
    ],
    "routes": [
      {
        "src": "static/api_home/(.*)",
        "dest": "static/api_home/$1"
      },
      {
        "src": "static/css/(.*)",
        "dest": "static/css/$1"
      },
      {
        "src": "/(.*)",
        "dest": "portfolio_Noura/wsgi.py"
      }
    ]
  }
  
  