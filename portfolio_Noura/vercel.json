{
    "builds": [
      {
        "src": "portfolio_Noura/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        }
      },

      {
        "src": "portfolio_Noura/build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "portfolio_Noura"
          }
      }
    ],
    "routes": [
      {
        "src": "static/(.*)",
        "dest": "static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "portfolio_Noura/wsgi.py"
      }
    ]
  }

  
